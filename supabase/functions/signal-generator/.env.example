# Environment variables for signal-generator Edge Function
# Copy this file to .env for local testing

# Supabase Configuration (automatically set in production)
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional: Rate Limiting
MAX_SIGNALS_PER_RUN=2
SCAN_INTERVAL_SECONDS=30

# Optional: Symbol Configuration
ENABLE_BTC=true
ENABLE_ETH=true
ENABLE_ALTCOINS=true
